      <option value="animals">Animals</option>
      <option value="chores">Chores</option>
      <option value="countries">Countries</option>
      <option value="sports">Sports</option>
      <option value="companies">Companies</option>
    </select>
    </div>
    <div class="lettersForGuessing"></div>
    <div class="word"></div>

    <canvas id="hangman" width="200" height="200">Your browser is not supported.</canvas>
  </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="assets/js/dependencies/main.js"></script>
    <script src="assets/js/dependencies/proto.js"></script>
